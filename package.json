{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"vite dev\" \"tsx watch server/index.ts\"",
    "dev:client": "vite dev",
    "dev:server": "tsx watch server/index.ts",
    "tailwind": "./bin/tailwindcss",
    "build:css": "./bin/tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "build:client": "npx vite build",
    "build:server": "npx esbuild server/index.ts --platform=node --bundle --format=cjs --external:@babel/preset-typescript --external:../pkg --external:fsevents --external:*.node --external:dotenv --external:dotenv/* --external:vite --external:vite/* --outfile=dist/index.cjs && mkdir -p dist && cp node_modules/connect-pg-simple/table.sql dist/",
    "build": "npm run build:client && npm run build:server",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node dist/index.cjs",
    "db:push": "npx dotenv-cli -e .env -- npx drizzle-kit push --config=./drizzle.config.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "npx dotenv-cli -e .env -- npx drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "npx dotenv-cli -e .env -- npx drizzle-kit migrate --config=./drizzle.config.ts",
    "db:run-migrations": "tsx ./server/migrate.ts",
    "db:seed": "tsx ./server/seed.ts",
    "db:test-insert": "tsx ./server/testDbOperations.ts",
    "db:test": "tsx ./server/testDbOperations.ts"
  },
